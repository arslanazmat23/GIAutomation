{
    "compilerOptions": {
      "target": "es2022",
      "lib": ["ES2022", "dom" , "DOM.Iterable"],
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["cypress", "@badeball/cypress-cucumber-preprocessor", "node"]
    },
    "include": [
      "cypress/**/*.ts",
      "**/*.cy.ts",
      "**/*.d.ts"
    ]
}