{
    "compilerOptions": {
      "target": "es6",
      "lib": ["es5", "dom"],
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["cypress", "@badeball/cypress-cucumber-preprocessor", "node"]
    },
    "include": [
      "cypress/**/*.ts",
      "**/*.cy.ts",
      "**/*.d.ts"
    ]
}